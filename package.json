{
  "name": "mobx-react-typescript-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for MobX + React project with Typescript, ES6, & HMR",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --hot --progress",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --bail --env=local",
    "build:staging": "webpack --config config/webpack.stag.js --progress --profile --bail",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "analyze": "npm run build:staging && open ./dist/report.html"
  },
  "author": "Jason Barnett <jason@thebarnetts.ws> (http://github.com/jhbarnett)",
  "repository": "https://github.com/jhbarnett/mobx-react-typescript-sass-boilerplate",
  "license": "MIT",
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/react": "^15.6.14",
    "@types/react-dom": "^15.5.7",
    "@types/react-loadable": "^5.3.3",
    "@types/react-router-dom": "^4.2.4",
    "awesome-typescript-loader": "^3.2.1",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "tslib": "^1.9.0",
    "typescript": "^2.4.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "semantic-ui-react": "^0.78.2"
  }
}
